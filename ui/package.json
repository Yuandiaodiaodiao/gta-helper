{
  "name": "gta-helper",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "viteinit": "vite",
    "start": "cross-env NODE_ENV=development electron .   ",
    "debug": "cross-env NODE_ENV=development electron .  --inspect ",
    "build-python": "pyinstaller ../src/main.py --clean --distpath ./pydist",
    "dist": "electron-builder",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "vite build",
    "buildall": "vite build &&  electron-builder"
  },
  "build": {
    "files": [
      "!node_modules/*"
    ],
    "appId": "com.yuandiaodiao.gtahelper",
    "asar": false,
    "directories": {
      "output": "build"
    },
    "win": {
      "icon": "favicon.png",
      "target": [
        "nsis",
        "zip"
      ],
      "extraResources": {
        "from": "./keyboardloop/",
        "to": "keyboardloop"
      }
    }
  },
  "author": "",
  "devDependencies": {
    "@vue/compiler-sfc": "^3.0.4",
    "cross-env": "^7.0.2",
    "electron": "^11.5.0",
    "electron-builder": "^22.9.1",
    "vite": "^1.0.0-rc.13"
  },
  "dependencies": {
    "element-plus": "^1.0.1-beta.10",
    "vue": "^3.0.4",
    "iconv-lite": "^0.6.2"
  }
}
